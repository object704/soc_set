<!DOCTYPE html>
<html>
    <head>
        <script src="{{ asset('bundles/userbundle/js/jquery.min.js')}}" type="text/javascript"></script>
        <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
        <title>{% block title %}Помощник в обучении!{% endblock %}</title>

        {% block stylesheets %}
            <link rel="stylesheet" href="{{ asset('bundles/userbundle/css/style-9.css') }}" type="text/css" media="all" />
            <link rel="stylesheet" href="{{ asset('bundles/userbundle/css/jq_start/jquery-ui-1.8.17.custom.css') }}" type="text/css"/>

            <script src="{{ asset('bundles/userbundle/js/jquery-1.7.1.min.js')}}" type="text/javascript"></script>
            <script src="{{ asset('bundles/userbundle/js/jquery-ui-1.8.17.custom.min.js')}}" type="text/javascript"></script>
            <script src="{{ asset('bundles/userbundle/js/jquery.bgiframe-2.1.2.js')}}" type="text/javascript"></script>

            <script src="{{ asset('bundles/userbundle/js/jquery.fancybox-1.2.1.pack.js')}}" type="text/javascript"></script>
            <script src="{{ asset('bundles/userbundle/js/changeFoto.js')}}" type="text/javascript"></script>
            <script src="{{ asset('bundles/userbundle/js/j_cookie.js')}}" type="text/javascript"></script>

            <script src="{{ asset('bundles/userbundle/js/chSpec.js')}}" type="text/javascript"></script>
            <script src="{{ asset('bundles/userbundle/js/trim.js')}}" type="text/javascript"></script>
            <script src="{{ asset('bundles/userbundle/js/searchPeople.js')}}" type="text/javascript"></script>
        {% endblock %}

    </head>
    <div id="title">
        <h1><a href="{{ path('prof') }}"></a>
        </h1>
    </div>
    <div id="exit">

        {% if is_granted("IS_AUTHENTICATED_REMEMBERED") %}
        <h4>{{ 'layout.logged_in_as'|trans({'%firstName%': app.user.firstName, '%lastName%': app.user.lastName}, 'FOSUserBundle') }} |
        <a href="{{ path('fos_user_security_logout') }}">
            {{ 'layout.logout'|trans({}, 'FOSUserBundle') }}
        </a>
        </h4>
        {% endif %}
    </div>

    {% block body %}

    {% endblock %}


    <div id="meta">
         <p>team/wise, &copy; {{ '' | date('Y') }}
                <a href="">Поддержка</a>
                <a href="">Тур по сайту</a>
                <a href="">Рассказать другу</a>
                <a href="">Пользовательское соглашение</a>

    </div>

</html>
